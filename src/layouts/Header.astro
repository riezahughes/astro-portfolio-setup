---
import { MainNavigation, ProfileIcon } from "@components";
import { AiFillLinkedin, AiFillGithub, AiFillHeart } from "react-icons/ai";
const menu = [
			{ url: "#", label: "Home" },
			{ url: "#articles", label: "Articles" },
			{ url: "#about", label: "About" },
			{ url: "#csv", label: "CSV" },
			{ url: "#contact", label: "Contact" }
		]
---
<div>
	<MainNavigation
		menu={menu}
	/>
	<nav id="media-accounts">
		<ProfileIcon title="linkedin" Icon={AiFillLinkedin} url="https://www.linkedin.com/in/dan-ruxton"/>
		<ProfileIcon title="github" Icon={AiFillGithub} url="https://github.com/riezahughes"/>
		<ProfileIcon title="heart" Icon={AiFillHeart} url="https://github.com/riezahughes"/>
	</nav>
</div>
 